{"response": "success",
  "result": {
    "flavors": [{% for flavor in flavors %}
      { 
        "id": "{{ flavor.key.id() }}",
        "name": "{{ flavor.name }}",
        "description": "{{ flavor.description }}",
        "vpus": {{ flavor.vpus }},
        "disk": {{ flavor.disk }},
        "memory": {{ flavor.memory }},
        "network": {{ flavor.network }},
        "rate": {{ flavor.rate }},
        "launches": {{ flavor.launches }},
        "flags": 0
      }{% if not loop.last %},{% endif %}{% endfor %}
    ]
  }
}
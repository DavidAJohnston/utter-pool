{"response": "success",
  "result": {
    "images": [{% for image in images %}
      { 
        "id": "{{ image.key.id() }}",
        "name": "{{ image.name }}",
        "description": "{{ image.description }}",
        "url": "{{ image.url }}",
        "size": {{ image.size }},
        "diskformat": "{{ image.diskformat }}",
        "containerformat": "{{ image.containerformat }}",
        "flags": 1
      }{% if not loop.last %},{% endif %}{% endfor %}]
  }
}